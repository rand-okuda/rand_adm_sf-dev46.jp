    <fields>
        <fullName>InspectionCompleted__c</fullName>
        <externalId>false</externalId>
        <formula>OR(

AND(
  RecordType.DeveloperName  = &apos;Solar&apos; ,
 NOT( ISPICKVAL(InspectionRresultA__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultB__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultC__c , &apos;&apos;) ) 
),
AND(
  RecordType.DeveloperName  = &apos;Air&apos; ,
 NOT( ISPICKVAL(InspectionRresultA__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultC__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultD__c , &apos;&apos;) ) 
),
AND(
  RecordType.DeveloperName  = &apos;Cogener&apos; ,
 NOT( ISPICKVAL(InspectionRresultA__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultB__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultD__c , &apos;&apos;) ) 
),
AND(
  RecordType.DeveloperName  = &apos;Storage&apos; ,
 NOT( ISPICKVAL(InspectionRresultA__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultD__c , &apos;&apos;) ) 
),
AND(
  RecordType.DeveloperName  = null ,
 NOT( ISPICKVAL(InspectionRresultA__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultB__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultC__c , &apos;&apos;) ) ,
 NOT( ISPICKVAL(InspectionRresultD__c , &apos;&apos;) ) 
)

)</formula>
